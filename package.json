{
	"name": "finhub",
	"private": true,
	"type": "module",
	"scripts": {
		"db:generate": "dotenv -- drizzle-kit generate --name",
		"db:generate-custom": "dotenv -- drizzle-kit generate --custom --name",
		"db:migrate": "dotenv -- drizzle-kit migrate",
		"db:drop": "dotenv -- drizzle-kit drop",
		"db:push": "dotenv -- drizzle-kit push",
		"db:studio": "dotenv -- drizzle-kit studio --port 6001 --verbose",
		"script": "tsx --env-file=.env",
		"dev": "node --env-file=.env server.js",
		"build": "react-router build",
		"start": "node --env-file=.env.build server.js",
		"email": "email dev --dir app/emails --port 6000",
		"typecheck": "react-router typegen && tsc -b",
		"format": "pnpm exec prettier . --write"
	},
	"dependencies": {
		"@conform-to/react": "^1.15.1",
